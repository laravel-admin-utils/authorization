{
    "name": "laravel-admin-utils/authorization",
    "description": "authorize utils of laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "utils", "authorization"],
    "homepage": "https://github.com/laravel-admin-utils/authorization",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaozhi",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "elegant-utils/laravel-admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\Utils\\Authorization\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elegant\\Utils\\Authorization\\AuthorizeServiceProvider"
            ]
        }
    }
}
